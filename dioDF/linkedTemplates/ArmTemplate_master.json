{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dioDF"
		},
		"DFBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DFBlobStorage'"
		},
		"DFSQLDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DFSQLDatabase'"
		},
		"uploadFile_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/549a47f8-014a-4ac9-a7e9-e32025993f10/resourceGroups/DataFactoryClass/providers/Microsoft.Storage/storageAccounts/diosa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dioDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DFBlobStorage_connectionString": {
						"value": "[parameters('DFBlobStorage_connectionString')]"
					},
					"DFSQLDatabase_connectionString": {
						"value": "[parameters('DFSQLDatabase_connectionString')]"
					},
					"uploadFile_properties_typeProperties_scope": {
						"value": "[parameters('uploadFile_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}